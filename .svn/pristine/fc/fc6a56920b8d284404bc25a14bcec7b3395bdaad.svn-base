    <script>
        $(function () {
            $(".header-menu li a").click(function () {
                var hr = $(this).attr("href");
                var anh = $(hr).offset().top;
                $("html,body").stop().animate({ scrollTop: anh }, 2000);
                $(this).parent().addClass("active").siblings().removeClass("active")
            });

            $(window).scroll(function () {
                if ($(this).scrollTop() > 80) {
                    // $(".bottom-wrap").show();
                    $(".header-wrap").addClass("fixed")
                } else {
                    // $(".bottom-wrap").hide();
                    $(".header-wrap").removeClass("fixed")
                }
            });

        })

    </script>
    <script>
        // 提交查询
        $('#fromSubmitQuery').on('submit', function(){
            submitQueryPost();
            event.preventDefault(); //阻止form表单默认提交
        });
        function submitQueryPost () {
            var submitQueryNumber = $("#submitQueryNumber").val();
            if( !isMobileNumber( submitQueryNumber ) )
                return;
            $.ajax({
                type: "post",
                url: "/clientSubmit/submitQuery",
                data: $('#fromSubmitQuery').serialize(),
            }).success(function(message) {
                console.log(message);
                $('#closeBtn').click();
                // document.getElementById("submitQuery").style.display="none";
                if (message === 'ok')
                    alert("提交成功");
                else {
                    alert("提交失败");
                }
            }).fail(function(err){
                alert("提交失败");
            });
        }

        function submitEnterPost () {
            var phoneNumber = $("#youPhone").val();
            if( !isMobileNumber( phoneNumber ) )
                return;
            var jsonData = {
                userName: $("#youName").val(),
                youPhone: phoneNumber,
                youWX: $("#youWX").val(),
                youDescription: $("#youDescription").val()
            };

            $.ajax({
                type: "post",
                url: "/clientSubmit/submitEnter",
                data: jsonData,
            }).success(function(message) {
                console.log(message);
                if (message === 'ok')
                    alert("提交成功");
                else {
                    alert("提交失败");
                }
            }).fail(function(err){
                alert("提交失败");
            });
        }

    </script>
