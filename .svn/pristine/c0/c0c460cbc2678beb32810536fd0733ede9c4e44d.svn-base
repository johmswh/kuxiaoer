body{font-family:微软雅黑,Microsoft YaHei,\5FAE\8F6F\96C5\9ED1;font-size:12px;_background-attachment:fixed;color:#999999}
body {background:#282d57; padding-bottom:40px;min-width: 1200px;}
html {_overflow-x:hidden}
div,ul,li,dl,dt,dd,table,td,input{font-size:12px;}
h1,h2,h3,h4,h5,h6{font-weight:normal;font-size:12px;}
i,em {font-style:normal;}
.clear {clear:both;zoom:1;}
.clear:after,.clear:before {clear:both;display:block;content:"";height:0;width:100%;font-size:0;visibility:hidden;zoom:1;}
.box { width: 1200px; margin: 0 auto;font-family:microsoft yahei; clear: both; position: relative;}
/*内容开始*/
.header-wrap {width:100%;height:82px;background-color:#282d57;z-index:91}
.header-wrap.fixed{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
}
.nav{width:73px;height:100%;position:fixed;top:20%;left:150px; }
.nav .top{width: 100%;height:73px;border-radius:5px;background-color: #c9121c;line-height: 34px;}
.nav .top a:hover{cursor:pointer;}
.nav .top span{
    font-size:14px;
    color:#fff;
    text-align: center;
}
.nav li{width:68px;height: 63px;text-align: center; border-width:0 1px;border-bottom:1px dashed #e0e0e0;margin-left: 2px;background-color: #fff;}
.nav li a{
    width: 68px;
    font-family:arial;
    height: 63px;
    font-size:18px;
    line-height: 26px;
    color:#8C8C8C;
    text-align: center;
    display: inline-block;
}
.nav li a span{
    line-height: 65px;
}

.nav li  a:hover {color:#fff;background-color: #282d57;}
/*.nav li.active a {background-color: #282d57;color:#fff}*/
.header {height:82px;}
.header .logo {display:block;width:194px;height:100%;background:url(../../images/entertwo/logo.png) no-repeat center;position:absolute;left:0;top:0;z-index:2}
.header .header-tel{width: 200px;float: right;margin-top: 20px;}
.header .header-telleft{width: 50px;float: left;}
.header .header-telright{width: 150px;float: right;font-size:16px;color:#fff;}

.header-contact {position:absolute;right:129px;top:21px;z-index:2}
