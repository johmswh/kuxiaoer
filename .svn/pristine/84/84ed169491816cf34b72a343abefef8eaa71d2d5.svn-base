
function setCenterHeightPX() {
    var contentInfo = document.getElementById( "contentInfo" );
    var contentBGTop = document.getElementById( "contentBGTop" );

    var companyInfo = document.getElementById( "companyInfo" );
    var ciHeight = companyInfo.offsetHeight + 40;
    contentInfo.style.height = ciHeight + 188 + 'px';
    contentBGTop.style.height = ciHeight + 'px';

}